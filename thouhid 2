for i in range(1, 100):
    if i % 2 == 0:
        continue  # Skip even numbers, go to next iteration
         # Exit the loop when i becomes 25
          # Explicitly do nothing (dummy placeholder)

    # This block runs only for odd numbers before 25
    print(i)
